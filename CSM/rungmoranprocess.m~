% parameterspopSize= 1000;initialDistribution = [3800000,380000, 21, 15, 8, 0.5, 0 ];initialDistribution = initialDistribution ./ sum(initialDistribution);trans = zeros(7,7);trans(1,1)= 1/3;trans(1,6)= 1/3;trans(1,7) = 1/3;trans(2,1) = 1/2;trans(2,2)= 1/2;trans(3,2) = 1/2;trans(3,3) = 1/2;trans(4,3) = 1/2;trans(4,4) = 1/2;trans(5,5) = 1;trans(6,4) = 1/2;trans(6,6) = 1/2;trans(7,1) = 1/6;trans(7,2) = 1/6;trans(7,3) = 1/6;trans(7,4) = 1/6;trans(7,6) = 1/6;trans(7,7) = 1/6;hold onnumStates = length(initialDistribution);colors = hsv2rgb([((0:1:numStates-1)')./numStates, ones(numStates, 1),ones(numStates,1).*0.75]);numIter = 365*10;%plottingresult = generalMoranProcess(trans, popSize, initialDistribution, numIter );    popTimeSeries = zeros(numStates, numIter);%     for state = 1:numStates%             popTimeSeries(state,:) = sum(result == state,1);%             plot(popTimeSeries(state,:),'Color', colors(state,:));%     end%             for state = 1:numStates            popTimeSeries(state,:) = sum(result == state,1);            plot(popTimeSeries(state,:),'Color', colors(state,:), 'LineWidth', 2);    end    legend('S','E','I1','I2','I3','P','T');    title(['Markov Chain Simulation with Initial Distribution [S,E,I1,I2,I3,P,T]=[3800000,380000, 21, 15, 8, 0.5, 0 ]']);    